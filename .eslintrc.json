{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5
  },
  "globals": {
    "export": true,
    "define": false
  },
  "rules": {
    "semi": ["warn", "always"],
    "quotes": ["warn", "single"],
    "no-debugger": "error"
  }
}
