{
    "name": "code-realm",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": " rollup -c --watch",
        "lint": "eslint **/*.js --fix",
        "format": "prettier-eslint --write $PWD/'src/**/*.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": [
            "npm run lint",
            "npm run format",
            "git add"
        ]
    },
    "keywords": [
        "modernjs",
        "practice"
    ],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "eslint": "^7.7.0",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.11",
        "prettier-eslint": "^11.0.0",
        "prettier-eslint-cli": "^5.0.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.26.5",
        "rollup-plugin-node-resolve": "^5.2.0"
    }
}